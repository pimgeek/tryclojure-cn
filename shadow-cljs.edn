{:deps {:aliases [:dev]}
 :dev-http {8200 "resources/public"}

 :builds
 {:app {:target :browser
        :output-dir "resources/public/js"
        :asset-path "/js"
        :module-hash-names false
        :modules {:app {:entries [app.core]}}
        :dev {:closure-defines {app.env/DEBUG true}}
        :build-hooks [(shadow.cljs.build-report/hook)]}}}
